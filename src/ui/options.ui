QCheckBox::indicator {
    width: 12px;
    height: 12px;
    border: 4px solid #202025; /* Makes the checkmark fill smaller */
    border-radius: 4px;
    background: #202020;
}

QCheckBox::indicator:checked {
    width: 12px;
    height: 12px;
    background: #7070BB;
    border: 4px solid #202025; /* Makes the checkmark fill smaller */
    border-radius: 3px;
}


/* SpinBox */
QSpinBox {
    background-color: #282838;
    color: #E0E0E0;
    border: 2px solid #5A5A8E;
    border-radius: 4px;
    min-height: 22px;
    font-size: 12px;
    padding: 2px;
}

